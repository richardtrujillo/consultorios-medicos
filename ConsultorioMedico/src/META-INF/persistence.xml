<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="ConsultorioMedicoPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>consultoriomedico.CTmedicoeps</class>
    <class>consultoriomedico.CTeps</class>
    <class>consultorio.VO.CTmedicoeps</class>
    <class>consultorio.VO.CTeps</class>
    <class>consultorio.VO.CTConsulta</class>
    <class>consultorio.VO.CTTipoPaciente</class>
    <class>consultorio.VO.CTPaciente</class>
    <class>consultorio.VO.CTMedicoprofesional</class>
    <class>consultorio.VO.CTValoracion</class>
    <class>consultorio.VO.CTipoPaciente</class>
    <class>consultorio.VO.CConsulta</class>
    <class>consultorio.VO.CPaciente</class>
    <class>consultorio.VO.THistoriaClinica</class>
    <class>consultorio.VO.CValoracion</class>
    <class>consultorio.VO.CMedicoprofesional</class>
    <class>consultorio.VO.CMedicoEps</class>
    <class>consultorio.VO.CEps</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/consultorio?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
